                    <div id="filterResultsFormHolder columns twelve">
                        <form name="filterResultsForm" id="filterResultsForm" action="/blog/filterPages/" method="post" {{ form_enctype(filterForm) }} class="filterForm">
                            <div class="column six">  
                                {{ form_row(filterForm.tags, { 'attr': {'class': 'customCheckBox'} }) }}
                            </div>
                            <div class="column six">  
                                {{ form_row(filterForm.categories, { 'attr': {'class': 'customCheckBox'} }) }}
                            </div>
                            <div class="column twelve">  
                                {{ form_rest(filterForm) }}
                                <input type="submit" id="filterResultsBtn" value="Search" />
                                {{ form_errors(filterForm) }}
                                <input class="checkAllBtn" type="button" id="CheckAll" name="CheckAll" value="Check All" />
                            </div>
                        </form>
                     </div>