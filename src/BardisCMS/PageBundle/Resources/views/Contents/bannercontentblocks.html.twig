{% for contentBlock in page.bannercontentblocks|sort_by_attribute('ordering') %}
    {% if contentBlock.publishedState == 1 %}
		<div class="contentBlock {% if contentBlock.className is not null %}{{ contentBlock.className }}{% endif %} large-12 small-12" {% if contentBlock.idName is not null %}id="{{ contentBlock.idName }}"{% endif %}>
			{% include 'PageBundle:Contents:bannercontentblock.html.twig' %}
		</div>
    {% endif %}
{% endfor %}
