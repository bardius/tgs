	<dd>
		<a class="spotAttributeTitle displayActiveStatus" href="#lifestyleTab">Lifestyle</a>	
		<div id="lifestyleTab" class="row collapse spotAttributeRow content">
			<div class="columns large-2 small-12">
				<h5>Nightlife</h5>
				<span>{{ page.spotAttributes.nightlife|booleanToString }}</span>
			</div>
			<div class="columns large-2 small-12">
				<h5>Family</h5>
				<span>{{ page.spotAttributes.family|booleanToString }}</span>
			</div>
			<div class="columns large-2 small-12">
				<h5>Restaurants</h5>
				<span>{{ page.spotAttributes.restaurants|booleanToString }}</span>
