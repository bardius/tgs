{
	"name": "symfony/framework-standard-edition",
	"description": "The \"Symfony Standard Edition\" distribution",
	"autoload": {
		"psr-0": {"": "src/"}
	},
	"require": {
		"php": ">=5.4.3",
		"symfony/symfony": "2.4.*",
		"doctrine/orm": ">=2.3.0",
		"doctrine/doctrine-bundle": "dev-master",
		"doctrine/doctrine-fixtures-bundle": "dev-master",
		"doctrine/data-fixtures" : "dev-master",
		"twig/extensions": "dev-master",
		"symfony/assetic-bundle": "dev-master",
		"symfony/swiftmailer-bundle": "dev-master",
		"symfony/monolog-bundle": "2.4.*",
		"sensio/distribution-bundle": "dev-master",
		"sensio/framework-extra-bundle": ">=2.2.0",
		"sensio/generator-bundle": "dev-master",
		"jms/security-extra-bundle": "dev-master",
		"jms/di-extra-bundle": "dev-master",
		"friendsofsymfony/user-bundle": ">=1.3.3",
		"sonata-project/jquery-bundle": ">=1.8.3",
		"sonata-project/admin-bundle": "2.2.7",
		"sonata-project/exporter": ">=1.3.1",
		"sonata-project/block-bundle": "dev-master",
		"sonata-project/cache-bundle": "dev-master",
		"sonata-project/media-bundle": "dev-master",
		"sonata-project/user-bundle": "dev-master",
		"sonata-project/doctrine-orm-admin-bundle": "dev-master",
		"facebook/php-sdk": "dev-master",
		"simplethings/entity-audit-bundle": "dev-master",
		"stfalcon/tinymce-bundle": "dev-master",
		"pimple/pimple": "dev-master",
		"leafo/scssphp": "dev-master",
		"leafo/scssphp-compass": "dev-master",
		"sonata-project/intl-bundle": "2.1.*"
	},
	"scripts": {
		"post-install-cmd": [
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		],
		"post-update-cmd": [
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		]
	},
	"config": {
		"bin-dir": "bin"
	},
	"minimum-stability": "dev",
	"extra": {
		"symfony-app-dir": "app",
		"symfony-web-dir": "web"
	}
}
